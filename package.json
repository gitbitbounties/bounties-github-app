{
  "name": "git-bit",
  "version": "1.0.0",
  "description": "Bitcoin rewards for Github",
  "author": "Joe Forsmann <josephfo@microsoft.com> (http://localhost:1234)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com//bitcoin-rewards.git"
  },
  "homepage": "https://github.com//bitcoin-rewards",
  "bugs": {
    "url": "https://github.com//bitcoin-rewards/issues"
  },
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@types/nock": "^9.3.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "express": "^4.17.1",
    "nock": "^10.0.0",
    "probot": "^7.2.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^1.17.2",
    "react-scripts": "^3.0.1",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "main": "jest.config.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
